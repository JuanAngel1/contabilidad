        print("8.-Presupuesto de Gastos Indirectos de Fabricacion")
        print("1 semestre")
        depreciacion = int(input("Ingresa la depreciacion durante el 1 semestre: "))
        seguro= int(input("Ingresa la cantidad de seguros: "))
        mantenimiento = int(input("Ingresa la cantidad de Mantenimiento: "))
        energeticos = int(input("Ingresa la cantidad de Energeticos: "))
        varios = int(input("Ingresa la cantidad de Varios: "))
