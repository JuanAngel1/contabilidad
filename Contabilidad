        costo_determinacione = float(input("Ingrese el costo: "))
        cantidad_determinacione = float(input("Ingrese la cantidad: "))
        print(f"El costo unitario de gastos indirectos de fabricacion es: {costo_determinacione*cantidad_determinacione}")
        costo_unitarioA = costo_determinacion * cantidad_determinacion
        costo_unitarioB = costo_determinacionb * cantidad_determinacionb
        costo_unitarioC = costo_determinacionc * cantidad_determinacionc
        costo_unitarioD = costo_determinaciond * cantidad_determinaciond
        costo_unitarioE = costo_determinacione * cantidad_determinacione

        print(f"El costo unitario es: {costo_unitarioA+costo_unitarioB+costo_unitarioC+costo_unitarioD+costo_unitarioE}")
        print("")
        print("")

        print("Producto 2")
        print("Material A")
