        print("11.- Valuacion de Inventarios Finales")
        print("Inventario Final de Materiales")
        print("")
        print("Material A")
        unidadesa = float(input("Ingresa las unidades del material A:"))
        unitario_costo = float(input("Ingresa el costo unitario: "))
        total_valuacion1 = unidadesa * unitario_costo
        print(f"El costo total del material A fue: {total_valuacion1}")
        print("")
        print("Material B")
        unidadesb = float(input("Ingresa las unidades del material B:"))
        unitario_costob = float(input("Ingresa el costo unitario: "))
        total_valuacion2 = unidadesb * unitario_costob
        print(f"El costo total del material B fue: {total_valuacion2}")
        print("")
        print("Material C")
        unidadesc = float(input("Ingresa las unidades del material C:"))
        unitario_costoc = float(input("Ingresa el costo unitario: "))
        total_valuacion3 = unidadesc * unitario_costoc
        print(f"El costo total del material C fue: {total_valuacion3}")
        print("")
        inventario_final_de_materiales = total_valuacion1 + total_valuacion2 + total_valuacion3
        print(f"El inventario final de materiales fue: {inventario_final_de_materiales}")
        print("")
        print("")
        print("Inventario Final de Producto Terminado")
        print("Producto 1")
        unidades_final1 = float(input("Ingresa las unidades del producto 1: "))
        costo_uni_final1 = float(input("Ingresa el costo unitario del producto 1: "))
        costo_total1 = unidades_final1 * costo_uni_final1
        print(f"El costo total del producto 1 fue: {costo_total1}")
        print("")
        print("Producto 2")
        unidades_final2 = float(input("Ingresa las unidades del producto 2: "))
        costo_uni_final2 = float(input("Ingresa el costo unitario del producto 2: "))
        costo_total2 = unidades_final2 * costo_uni_final2
        print(f"El costo total del producto 1 fue: {costo_total2}")
        print("")
        print("Producto 3")
        unidades_final3 = float(input("Ingresa las unidades del producto 1: "))
        costo_uni_final3 = float(input("Ingresa el costo unitario del producto 1: "))
        costo_total3 = unidades_final3 * costo_uni_final3
        print(f"El costo total del producto 1 fue: {costo_total3}")
        print("")
        print(f"Inventario Final de Producto Terminado: {costo_total1+costo_total2+costo_total3}")
        print("")
        print("")
        print("***************************************************************************")
        print("")
        print("PRESUPUESTO FINANCIERO")
        print("Estado de Costo de Produccion y Ventas")
        print("Presupuesto del 1 de Enero al 31 de diciembre del 2016")
        saldo_inicial_materiales = float(input("Ingresa el saldo inicial de materiales: "))
        print(suma_totales)
        compras_materiales = float(input("Ingresa la cantidad de compras de materiales: "))
        material_disponible = saldo_inicial_materiales+compras_materiales
        print(f"El material disponible fue {material_disponible}")
        print(f"(-) Inventario Final de materiales: {inventario_final_de_materiales}")
        print(f"(=) Los materiales utilizados fueron: {material_disponible-inventario_final_de_materiales}")
        print(f"(+) Mano de obra directa: {total_mod+total_mod2}")
        print(f"(+) Gastos de fabricacion indirectos: {gif}")
        material_utilizado = material_disponible-inventario_final_de_materiales
        mano_obra_directa = total_mod+total_mod2
        print(f"Costo de produccion: {material_utilizado+mano_obra_directa+gif}")
        costo_produccion = material_utilizado+mano_obra_directa+gif
        InventarioInicialdeProductosTerminado = float(input("Ingresa la cantidad de Inventario Inicial de Productos Terminados: "))
        print(f"(=) El total de produccion disponible es {costo_produccion+InventarioInicialdeProductosTerminado}")
        total_produccion_disponible = costo_produccion+InventarioInicialdeProductosTerminado
        inventario_final_productos_terminado = costo_total1+costo_total2+costo_total3
        print(f"(-) El inventario Final de Productos Terminados es: {inventario_final_productos_terminado}")
        print(f"El costo de ventas fue : {total_produccion_disponible-inventario_final_productos_terminado}")
        print("")
        print("")
        print("*******************************************************")

        print("Estado de Resultados")
        print("Presupuesto del 1 de Enero al 31 de Diciembre del 2016")
        print("")
        vendius = importe+importe2+importe3+importe4+importe5+importe6
        print(f"Ventas : {vendius}")
        costo_ventas = total_produccion_disponible-inventario_final_productos_terminado
        print(f"(-) Costo de ventas: {costo_ventas}")
        print(f"(=) Utilidad bruta: {vendius-costo_ventas}")
        utilidad_bruta = vendius-costo_ventas
        gasto_operacion = total_gasto_operacion+total_gasto_operacion2
        print((f"(-) Gastos de operacion: {gasto_operacion}"))
        print(f"Utilidad de operacion: {utilidad_bruta-gasto_operacion}")
