        isr = float(input("Ingrese el ISR de acuerdo a la redaccion: "))
        print(f"(-) ISR: {isr}")
        ptu = float(input("Ingrese el PTU de acuerdo a la redaccion: "))
        print(f"(-) PTU: {ptu}")
        print(f"(=) Utilidad Neta: {utilidad_operacion-isr-ptu}")
        print("")
        print("")
        print("**************************************************************************")
        print("")
        print("Estado de Flujo de Efectivo")
        print("")
        saldo_inicial_efectivo = float(input("Ingrese el Saldo Inicial de Efectivo: "))
        print(f"Saldo Inicial de Efectivo: {saldo_inicial_efectivo}")
        print("ENTRADAS")
        print(f"Cobranza 2016: {cobranza} ")
        print(f"Cobranza 2015: {saldo_cliente}")
        print(f"Total de entradas: {cobranza+saldo_cliente}")
        total_entradas_flujo = cobranza+saldo_cliente
        print(f"Efectivo disponible: {total_entradas_flujo+saldo_inicial_efectivo}")
        total_entradas_entradas = total_entradas_flujo+saldo_inicial_efectivo
        print("")
        print("")
        print("Salidas")
        print(f"Proovedores 2016: {proovedores_2016_50} ")
        print(f"Proovedores 2015: {saldo_proovedores}")
        pago_mano_obra = total_mod+total_mod2
        print(f"Pago Mano de Obra Directa: {pago_mano_obra}")
        pago_indirecto_fabricacion= gif-total_depreciacion
        print(f"Pago Gastos Indirectos de Fabricacion: {pago_indirecto_fabricacion}")
        gasto_operacion = depreciacion_operacion+depreciacion_operacion2
        pago_gasto_operacion = total_gasto_operacion+total_gasto_operacion2
        combi_operacion = pago_gasto_operacion - gasto_operacion
        print(f"Pago de Gastos de Operacion: {combi_operacion}")
        maquinaria = float(input("Ingrese Compra de Activo Fijo: "))
