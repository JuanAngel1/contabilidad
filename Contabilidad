        print("10.- Determinacion del Costo Unitario de Productos Terminados")
        print("Producto 1")
        print("Material A")
        costo_determinacion = float(input("Ingrese el costo: "))
        cantidad_determinacion = float(input("Ingrese la cantidad: "))
        print(f"El costo unitario del material A es: {costo_determinacion*cantidad_determinacion}")
        print("")
        print("Material B")
        costo_determinacionb = float(input("Ingrese el costo: "))
        cantidad_determinacionb = float(input("Ingrese la cantidad: "))
        print(f"El costo unitario del material B es: {costo_determinacionb*cantidad_determinacionb}")
        print("")
        print("Material C")
        costo_determinacionc = float(input("Ingrese el costo: "))
        cantidad_determinacionc = float(input("Ingrese la cantidad: "))
        print(f"El costo unitario del material C es: {costo_determinacionc*cantidad_determinacionc}")
        print("")
        print("Mano de obra")
        costo_determinaciond = float(input("Ingrese el costo: "))
        cantidad_determinaciond = float(input("Ingrese la cantidad: "))
